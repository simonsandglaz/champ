extends ../question

block question_category
	span Physical Health

block question
	h4 During the past 30 days, how often have you had 5 or more drinks on one occasion 

block answers
	div(class='form-check')
		input(class='form-check-input' type='radio' id='radio1' name='answer' value='option1')
		label(class='form-check-label' for='radio1') Daily or almost daily
	div(class='form-check')
		input(class='form-check-input' type='radio' id='radio2' name='answer' value='option2')
		label(class='form-check-label' for='radio4') 4 or 5 times a week
	div(class='form-check')
		input(class='form-check-input' type='radio' id='radio3' name='answer' value='option3')
		label(class='form-check-label' for='radio4') 2 or 3 times a week
	div(class='form-check')
		input(class='form-check-input' type='radio' id='radio4' name='answer' value='option4')
		label(class='form-check-label' for='radio4') Once a week
	div(class='form-check')
		input(class='form-check-input' type='radio' id='radio5' name='answer' value='option5')
		label(class='form-check-label' for='radio5') 2 to 3 times in the past 30 days
	div(class='form-check')
		input(class='form-check-input' type='radio' id='radio6' name='answer' value='option6')
		label(class='form-check-label' for='radio6') Once in the past 30 days
	div(class='form-check')
		input(class='form-check-input' type='radio' id='radio6' name='answer' value='option6')
		label(class='form-check-label' for='radio6') Not in the past 30 days